{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "useTypes": {
      "type": "string",
      "default": "field",
      "required": true
    },
    "palettes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::palette.palette",
      "inversedBy": "blogs"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "field": {
      "type": "string",
      "default": "extend.parts.hair",
      "required": false
    },
    "markdown": {
      "type": "richtext"
    },
    "content": {
      "type": "blocks"
    },
    "tools": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tool.tool",
      "inversedBy": "blogs"
    },
    "views": {
      "type": "integer",
      "default": 0
    },
    "moreBlogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "inversedBy": "blogs"
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "moreBlogs"
    }
  }
}
