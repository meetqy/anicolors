{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "palettes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::palette.palette",
      "inversedBy": "blogs"
    },
    "content": {
      "type": "blocks",
      "required": false
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "data": {
      "type": "json"
    },
    "field": {
      "type": "string",
      "private": true,
      "required": true
    }
  }
}
